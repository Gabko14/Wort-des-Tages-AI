{
  "expo": {
    "name": "Wort des Tages",
    "slug": "Wort-des-Tages-AI",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "wortdestagesai",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/adaptive-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#64b5f6"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.wortdestagesai.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundImage": "./assets/images/adaptive-icon-background.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-updates",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "react-native",
          "organization": "gibb-jw"
        }
      ],
      "expo-iap",
      "@react-native-community/datetimepicker"
    ],
    "assetBundlePatterns": ["assets/database/*"],
    "experiments": {
      "typedRoutes": true
    },
    "updates": {
      "url": "https://u.expo.dev/0755056e-ba3b-40fa-a430-20efc3173c4c"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0755056e-ba3b-40fa-a430-20efc3173c4c"
      }
    }
  }
}
